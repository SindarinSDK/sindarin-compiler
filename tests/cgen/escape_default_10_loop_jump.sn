// Code Generation Test: 10 Nested Loop Arena Escape (Jump)
//
// Tests that string values can jump directly from innermost loop (depth 11)
// to any outer scope, skipping intermediate arenas.

fn build_with_jumps(): str =>
    var to_d1: str = ""
    for var a: int = 1; a <= 1; a++ =>
        var to_d2: str = ""
        for var b: int = 1; b <= 1; b++ =>
            var to_d3: str = ""
            for var c: int = 1; c <= 1; c++ =>
                var to_d4: str = ""
                for var d: int = 1; d <= 1; d++ =>
                    var to_d5: str = ""
                    for var e: int = 1; e <= 1; e++ =>
                        var to_d6: str = ""
                        for var f: int = 1; f <= 1; f++ =>
                            var to_d7: str = ""
                            for var g: int = 1; g <= 1; g++ =>
                                var to_d8: str = ""
                                for var h: int = 1; h <= 1; h++ =>
                                    var to_d9: str = ""
                                    for var i: int = 1; i <= 1; i++ =>
                                        var to_d10: str = ""
                                        for var j: int = 1; j <= 1; j++ =>
                                            // All jumps originate from depth 11
                                            to_d10 = "d11->d10"
                                            to_d9 = "d11->d9"
                                            to_d8 = "d11->d8"
                                            to_d7 = "d11->d7"
                                            to_d6 = "d11->d6"
                                            to_d5 = "d11->d5"
                                            to_d4 = "d11->d4"
                                            to_d3 = "d11->d3"
                                            to_d2 = "d11->d2"
                                            to_d1 = "d11->d1"
                                        to_d9 = to_d9 + " | at_d10:" + to_d10
                                    to_d8 = to_d8 + " | at_d9:" + to_d9
                                to_d7 = to_d7 + " | at_d8:" + to_d8
                            to_d6 = to_d6 + " | at_d7:" + to_d7
                        to_d5 = to_d5 + " | at_d6:" + to_d6
                    to_d4 = to_d4 + " | at_d5:" + to_d5
                to_d3 = to_d3 + " | at_d4:" + to_d4
            to_d2 = to_d2 + " | at_d3:" + to_d3
        to_d1 = to_d1 + " | at_d2:" + to_d2
    return to_d1

fn main(): int =>
    println("Jump escape test (10 loops):")
    println("Values jump directly from d11 to each target depth")
    println(build_with_jumps())
    println("PASS: All jump escapes succeeded")
    return 0
