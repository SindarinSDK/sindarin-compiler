// Test 20: Operator combinations and precedence
fn main(): int =>
    print("=== Arithmetic Precedence ===\n")
    print($"2 + 3 * 4 = {2 + 3 * 4}\n")
    print($"(2 + 3) * 4 = {(2 + 3) * 4}\n")
    print($"2 * 3 + 4 = {2 * 3 + 4}\n")
    print($"2 * (3 + 4) = {2 * (3 + 4)}\n")
    print($"10 / 2 * 5 = {10 / 2 * 5}\n")
    print($"10 - 5 - 2 = {10 - 5 - 2}\n")
    print($"10 - (5 - 2) = {10 - (5 - 2)}\n")

    print("\n=== Division and Modulo ===\n")
    print($"17 / 5 = {17 / 5}\n")
    print($"17 % 5 = {17 % 5}\n")
    print($"17 / 5 * 5 + 17 % 5 = {17 / 5 * 5 + 17 % 5}\n")

    print("\n=== Comparison Operators ===\n")
    var a: int = 5
    var b: int = 10
    print($"a < b: {a < b}\n")
    print($"a > b: {a > b}\n")
    print($"a <= 5: {a <= 5}\n")
    print($"a >= 5: {a >= 5}\n")
    print($"a == 5: {a == 5}\n")
    print($"a != b: {a != b}\n")

    print("\n=== Logical Operators ===\n")
    print($"true && true: {true && true}\n")
    print($"true && false: {true && false}\n")
    print($"true || false: {true || false}\n")
    print($"false || false: {false || false}\n")
    print($"!true: {!true}\n")
    print($"!false: {!false}\n")

    print("\n=== Combined Comparison and Logical ===\n")
    print($"(a < b) && (b < 20): {(a < b) && (b < 20)}\n")
    print($"(a > b) || (b < 20): {(a > b) || (b < 20)}\n")
    print($"!(a > b): {!(a > b)}\n")
    print($"a < b && b < 20: {a < b && b < 20}\n")

    print("\n=== Complex Expressions ===\n")
    var x: int = 5
    var y: int = 3
    var z: int = 2
    print($"x + y * z: {x + y * z}\n")
    print($"(x + y) * z: {(x + y) * z}\n")
    print($"x * y + z: {x * y + z}\n")
    print($"x * (y + z): {x * (y + z)}\n")

    print("\n=== Unary Minus ===\n")
    print($"-5: {-5}\n")
    print($"-(-5): {-(-5)}\n")
    print($"3 + -2: {3 + -2}\n")
    print($"3 - -2: {3 - -2}\n")
    print($"3 * -2: {3 * -2}\n")

    print("\n=== Increment/Decrement ===\n")
    var n: int = 5
    n++
    print($"After n++: {n}\n")
    n--
    print($"After n--: {n}\n")
    n = n + 1
    print($"After n = n + 1: {n}\n")

    print("\n=== String Concatenation ===\n")
    var s1: str = "Hello"
    var s2: str = " "
    var s3: str = "World"
    print($"Concat: {s1 + s2 + s3}\n")

    return 0
