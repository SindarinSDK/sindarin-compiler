# Test: Reverse a struct array and verify callbacks survive

@alias "rt_arena_v2_gc"
native fn gc_collect(arena): int

struct Entry =>
    key: str
    value: str

fn buildReversed(): Entry[] =>
    var items: Entry[] = { Entry { key: "a", value: "1" }, Entry { key: "b", value: "2" }, Entry { key: "c", value: "3" } }
    items.reverse()
    return items

fn main(): void =>
    var items: Entry[] = buildReversed()
    gc_collect()

    if items.length != 3 =>
        print($"FAIL: length {items.length}\n")
        return
    if items[0].key != "c" || items[0].value != "3" =>
        print("FAIL: item 0\n")
        return
    if items[1].key != "b" || items[1].value != "2" =>
        print("FAIL: item 1\n")
        return
    if items[2].key != "a" || items[2].value != "1" =>
        print("FAIL: item 2\n")
        return
    print("PASS\n")
