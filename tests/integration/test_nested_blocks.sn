// Test loop structures
fn main(): void =>
    // Simple for loop
    var sum: int = 0
    for var i: int = 1; i <= 10; i++ =>
        sum = sum + i
    print($"Sum 1 to 10: {sum}\n")

    // Another simple for loop
    var count: int = 0
    for var j: int = 0; j < 5; j++ =>
        count = count + 1
    print($"Count: {count}\n")
