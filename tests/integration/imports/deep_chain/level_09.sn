// Level 9 - imports level 10 with alias

import "./level_10" as level_10

static var staticCounter: int = 900
static var staticFlag: bool = true
var instanceCounter: int = 9

fn getStaticCounter(): int =>
    return staticCounter

fn getStaticFlag(): bool =>
    return staticFlag

fn toggleStaticFlag(): void =>
    staticFlag = !staticFlag

fn incrementStatic(): void =>
    staticCounter = staticCounter + 1

fn getInstanceCounter(): int =>
    return instanceCounter

fn getDeepStatic(): int =>
    return level_10.getStaticCounter()

fn getLevelId(): int =>
    return 9

fn computeChainValue(base: int): int =>
    return level_10.computeChainValue(base + 9)

fn verifyDeepDirectAccess(): bool =>
    return level_10.verifyDirectAccess()
