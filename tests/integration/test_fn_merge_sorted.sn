fn main(): void =>
    var a: int[] = {1, 3, 5, 7}
    var b: int[] = {2, 4, 6, 8}
    var merged: int[] = merge(a, b)
    print($"{merged.join(" ")}\n")

fn merge(a: int[], b: int[]): int[] =>
    var result: int[] = {}
    var i: int = 0
    var j: int = 0
    while i < a.length && j < b.length =>
        if a[i] <= b[j] =>
            result.push(a[i])
            i++
        else =>
            result.push(b[j])
            j++
    while i < a.length =>
        result.push(a[i])
        i++
    while j < b.length =>
        result.push(b[j])
        j++
    return result
