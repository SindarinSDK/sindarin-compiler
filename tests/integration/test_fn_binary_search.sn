fn main(): void =>
    var arr: int[] = {1, 3, 5, 7, 9, 11, 13}
    print($"{bin_search(arr, 7)}\n")
    print($"{bin_search(arr, 4)}\n")

fn bin_search(arr: int[], target: int): int =>
    var lo: int = 0
    var hi: int = arr.length - 1
    while lo <= hi =>
        var mid: int = (lo + hi) / 2
        if arr[mid] == target =>
            return mid
        if arr[mid] < target =>
            lo = mid + 1
        else =>
            hi = mid - 1
    return -1
