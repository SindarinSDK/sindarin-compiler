// Test string interpolation with struct that has custom toString()

struct Vector =>
    x: double
    y: double
    z: double

    fn toString(): str =>
        return $"<{self.x}, {self.y}, {self.z}>"

struct Color =>
    r: int
    g: int
    b: int

    fn toString(): str =>
        return $"rgb({self.r}, {self.g}, {self.b})"

fn main(): int =>
    // Test struct with custom toString
    var v: Vector = Vector { x: 1.5, y: 2.5, z: 3.5 }
    print($"Vector: {v}\n")

    // Test another struct with custom toString
    var c: Color = Color { r: 255, g: 128, b: 64 }
    print($"Color: {c}\n")

    // Test inline with custom toString
    print($"Inline: {Color { r: 0, g: 0, b: 0 }}\n")

    return 0
