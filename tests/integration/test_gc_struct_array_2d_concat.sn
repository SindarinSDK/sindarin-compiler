# Test: Concat two 2D struct arrays and GC

@alias "rt_arena_v2_gc"
native fn gc_collect(arena): int

struct Entry =>
    key: str
    value: str

fn buildConcat(): Entry[][] =>
    var r1: Entry[] = { Entry { key: "a", value: "1" } }
    var r2: Entry[] = { Entry { key: "b", value: "2" } }
    var g1: Entry[][] = { r1 }
    var g2: Entry[][] = { r2 }
    var combined: Entry[][] = g1.concat(g2)
    return combined

fn main(): void =>
    var grid: Entry[][] = buildConcat()
    gc_collect()

    if grid.length != 2 =>
        print($"FAIL: length {grid.length}\n")
        return
    if grid[0][0].key != "a" || grid[0][0].value != "1" =>
        print("FAIL: [0][0]\n")
        return
    if grid[1][0].key != "b" || grid[1][0].value != "2" =>
        print("FAIL: [1][0]\n")
        return
    print("PASS\n")
