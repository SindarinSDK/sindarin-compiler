fn main(): int =>
  // Test break
  print("Testing break:\n")
  var i = 0
  while i < 10 =>
    if i == 5 =>
      break
    print($"{i} ")
    i = i + 1
  print("\n")

  // Test continue
  print("Testing continue:\n")
  i = 0
  while i < 10 =>
    i = i + 1
    if i % 2 == 0 =>
      continue
    print($"{i} ")
  print("\n")

  // Test break in for loop
  print("Testing break in for:\n")
  for var j: int = 0; j < 10; j++ =>
    if j == 3 =>
      break
    print($"{j} ")
  print("\n")

  // Test continue in for-each
  print("Testing continue in for-each:\n")
  var nums = {1, 2, 3, 4, 5}
  for n in nums =>
    if n == 3 =>
      continue
    print($"{n} ")
  print("\n")

  return 0
