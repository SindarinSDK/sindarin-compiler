fn main(): void =>
    var arr: int[] = {4, 2, 7, 1, 3}
    for var i: int = 1; i < arr.length; i++ =>
        var key: int = arr[i]
        var j: int = i - 1
        while j >= 0 && arr[j] > key =>
            arr[j+1] = arr[j]
            j--
        arr[j+1] = key
    for var i: int = 0; i < arr.length; i++ =>
        print($"{arr[i]} ")
    print("\n")
