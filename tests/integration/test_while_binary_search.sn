fn main(): void =>
    var arr: int[] = {1, 3, 5, 7, 9, 11, 13}
    var target: int = 7
    var lo: int = 0
    var hi: int = arr.length - 1
    var found: int = -1
    while lo <= hi =>
        var mid: int = (lo + hi) / 2
        if arr[mid] == target =>
            found = mid
            break
        else if arr[mid] < target =>
            lo = mid + 1
        else =>
            hi = mid - 1
    print($"{found}\n")
