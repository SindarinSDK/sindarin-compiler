fn main(): void =>
    var arr: int[] = {2, 7, 11, 15}
    var target: int = 9
    var found: bool = false
    for var i: int = 0; i < arr.length; i++ =>
        for var j: int = i + 1; j < arr.length; j++ =>
            if arr[i] + arr[j] == target =>
                print($"{i} {j}\n")
                found = true
    if !found =>
        print("not found\n")
