fn main(): void =>
    var arr: int[] = {1, 2, 3, 4, 5, 6, 7, 8}
    var result: int = count_matching(arr, is_even_fn)
    print($"{result}\n")

fn is_even_fn(n: int): bool => n % 2 == 0
fn count_matching(arr: int[], pred: fn(int): bool): int =>
    var count: int = 0
    for x in arr =>
        if pred(x) =>
            count++
    return count
