fn main(): void =>
    var arr: int[] = {3, 7, 2, 8, 5}
    var result: int = find_first(arr, fn(x: int): bool => x > 6)
    print($"{result}\n")

fn find_first(arr: int[], pred: fn(int): bool): int =>
    for var i: int = 0; i < arr.length; i++ =>
        if pred(arr[i]) =>
            return arr[i]
    return -1
