# Test: Reverse a 3D struct array (outer dimension) and GC

@alias "rt_arena_v2_gc"
native fn gc_collect(arena): int

struct Entry =>
    key: str
    value: str

fn buildReversed(): Entry[][][] =>
    var r1: Entry[] = { Entry { key: "a", value: "1" } }
    var r2: Entry[] = { Entry { key: "b", value: "2" } }
    var l1: Entry[][] = { r1 }
    var l2: Entry[][] = { r2 }
    var cube: Entry[][][] = { l1, l2 }
    cube.reverse()
    return cube

fn main(): void =>
    var cube: Entry[][][] = buildReversed()
    gc_collect()

    if cube.length != 2 =>
        print($"FAIL: length {cube.length}\n")
        return
    if cube[0][0][0].key != "b" || cube[0][0][0].value != "2" =>
        print("FAIL: [0][0][0]\n")
        return
    if cube[1][0][0].key != "a" || cube[1][0][0].value != "1" =>
        print("FAIL: [1][0][0]\n")
        return
    print("PASS\n")
