// lib/math_ops.sn - Math operations for namespace testing

fn add(a: int, b: int): int =>
    return a + b

fn subtract(a: int, b: int): int =>
    return a - b

fn multiply(a: int, b: int): int =>
    return a * b

fn divide(a: int, b: int): int =>
    if b == 0 =>
        return 0
    return a / b
