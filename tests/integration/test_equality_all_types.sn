fn test_array_equality(): void =>
    print("Test: array equality\n")
    var a: int[] = {1, 2, 3}
    var b: int[] = {1, 2, 3}
    var c: int[] = {1, 2, 4}
    var d: int[] = {1, 2}

    assert(a == b, "int[] ==")
    assert(a != c, "int[] != (different element)")
    assert(a != d, "int[] != (different length)")

    var s1: str[] = {"hello", "world"}
    var s2: str[] = {"hello", "world"}
    var s3: str[] = {"hello", "there"}

    assert(s1 == s2, "str[] ==")
    assert(s1 != s3, "str[] !=")

    var b1: bool[] = {true, false, true}
    var b2: bool[] = {true, false, true}
    var b3: bool[] = {true, true, true}

    assert(b1 == b2, "bool[] ==")
    assert(b1 != b3, "bool[] !=")
    print("  PASS\n")

fn test_mixed_comparisons(): void =>
    print("Test: mixed type comparisons\n")

    // int and long comparison
    var i: int = 42
    var l: long = 42l
    assert(i == l, "int == long")

    // int and double comparison
    var d: double = 42.0
    assert(i == d, "int == double")

    // Expressions in comparisons
    assert((2 + 2) == 4, "expr == literal")
    assert((10 - 5) != 6, "expr != literal")
    assert((3 * 4) > 10, "expr > literal")

    print("  PASS\n")

fn main(): void =>
    print("=== Comprehensive Equality Tests ===\n\n")

    test_int_equality()
    test_long_equality()
    test_double_equality()
    test_float_equality()
    test_bool_equality()
    test_char_equality()
    test_byte_equality()
    test_string_equality()
    test_int32_equality()
    test_uint_equality()
    test_uint32_equality()
    test_array_equality()
    test_mixed_comparisons()

    print("\n=== All equality tests passed! ===\n")
